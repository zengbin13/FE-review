<view><scroll-view style="{{'height:'+(scrollHeight+'px')+';'}}" scroll-y data-event-opts="{{[['scrolltolower',[['init',['$event']]]]]}}" bindscrolltolower="__e"><view class="budget_item"><block wx:for="{{withdraw}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="budget_list"><view class="budget_top cl"><text class="type">提现</text><block wx:if="{{item.aliPayNo}}"><text class="num">{{"订单号:"+item.aliPayNo}}</text></block></view><view class="budget_itm"><text class="{{['p',(!item.remark)?'opacity':'']}}">{{"备注："+(item.remark||'')}}</text><text class="p">{{item.createTime}}</text></view><text class="price">{{item.amountReceived}}</text><block wx:if="{{item.withdrawStatus==0}}"><text class="status">待处理</text></block><block wx:if="{{item.withdrawStatus==1}}"><text class="{{['status',(item.withdrawStatus==1)?'succ':'']}}">提现成功</text></block><block wx:if="{{item.withdrawStatus==2}}"><text class="{{['status',(item.withdrawStatus==2)?'loser':'']}}">提现失败</text></block></view></block><load-more vue-id="1" loadingType="{{loadingType}}" bind:__l="__l"></load-more></view></scroll-view></view>