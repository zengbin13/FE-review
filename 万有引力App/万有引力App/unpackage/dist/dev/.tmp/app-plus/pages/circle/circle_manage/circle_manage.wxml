<view><view class="cirmanage"><view class="cirmanage_item"><view class="cirmanage_top"><view class="cl"><view class="cirmanage_head"><block wx:if="{{circleInfo.circleLogo}}"><image class="img" src="{{serviceImgUrl+circleInfo.circleLogo}}" mode="aspectFill"></image></block></view><view class="cirmanage_info"><text class="title">{{circleInfo.circleTitle}}</text><text class="num">{{userCount+"人 | "+dynamicCount+"篇主题"}}</text><text class="time">{{"创建于"+circleInfo.createTime}}</text></view></view><view class="cirmanage_content"><text class="title">圈子简介：</text>{{circleInfo.circleIntro+''}}</view></view><view class="cirmanage_itm"><view class="cirmanage_list"></view></view></view></view><view class="setUp"><block wx:if="{{status}}"><navigator class="setUp_list cl" url="{{'/pages/circleCreate/edit/edit?circleId='+circleId}}"><text class="name">编辑圈子资料</text><image class="img" src="/static/service-img/next_03.png"></image></navigator></block><navigator class="setUp_list cl" url="{{'/pages/circle/circle_user/circle_user?circleId='+circleId+'&rule='+rule}}"><text class="name">圈子成员</text><image class="img" src="../../../static/service-img/next_03.png"></image></navigator><navigator class="setUp_list cl" url="{{'/pages/circle/circle_manage/invitation_list/invitation_list?circleId='+circleId+'&isCreate='+circleInfo.isCreate}}"><text class="name">邀请榜</text><image class="img" src="../../../static/service-img/next_03.png"></image></navigator><block wx:if="{{status}}"><navigator class="setUp_list cl" url="{{'/pages/circle/circle_manage/circle_type/circle_type?circleId='+circleId+'&userId='+circleInfo.circleUserId+'&circleType='+circleInfo.circleType}}"><text class="name">成员加入方式</text><image class="img" src="../../../static/service-img/next_03.png"></image><text class="method">{{circleInfo.circleType==0?'免费':'付费'}}</text></navigator></block><block wx:if="{{status}}"><navigator class="setUp_list cl" url="{{'/pages/circleCreate/editRule/editRule?circleId='+circleId+'&rule='+circleInfo.circleReleaseRules}}"><text class="name">圈子权限设置</text><image class="img" src="../../../static/service-img/next_03.png"></image></navigator></block><block wx:if="{{rule==2}}"><navigator class="setUp_list cl" url="{{'/pages/invite_partner/invite_partner?circleId='+circleId}}"><text class="name">邀请合伙人</text><image class="img" src="../../../static/service-img/next_03.png"></image></navigator></block><block wx:if="{{status}}"><navigator class="setUp_list cl" url="{{'/pages/tg_setup/tg_setup?circleId='+circleId+'&shareReward='+circleInfo.shareReward+'&shareRewardImg='+shareRewardImg}}"><text class="name">推广设置</text><image class="img" src="../../../static/service-img/next_03.png"></image></navigator></block><view class="setUp_list cl"><text class="name">圈子认证类型</text><image class="img" src="../../../static/service-img/next_03.png"></image><text class="method">{{circleInfo.authLevel_desc}}</text></view><block wx:if="{{status==0}}"><view class="setUp_list cl"><text class="name">推广奖励</text><image class="img" src="../../../static/service-img/next_03.png"></image><text class="method">{{circleInfo.shareReward}}</text></view></block><block wx:if="{{rule!=2}}"><view data-event-opts="{{[['tap',[['exitTap',['$event']]]]]}}" class="setUp_list cl" bindtap="__e"><text class="name">退出圈子</text><image class="img" src="../../../static/service-img/next_03.png"></image></view></block><block wx:if="{{circleInfo.isCreate}}"><view data-event-opts="{{[['tap',[['delTap',['$event']]]]]}}" class="setUp_list cl" bindtap="__e"><text class="name">删除圈子</text><image class="img" src="../../../static/service-img/next_03.png"></image></view></block></view></view>