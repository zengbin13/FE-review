
.attestation_top {
	padding: 17rpx 30rpx 0;
}
.attestation_top .title {
	float: left;
	color: #333;
	display: block;
	font-size: 28rpx;
}
.attestation_top .current {
	color: #333;
	float: right;
	display: block;
	font-size: 28rpx;
}
.attestation_top .current .status {
	color: #999;
	font-size: 28rpx;
}
.attestation_top .current .img {
	top: 3rpx;
	width: 16rpx;
	height: 26rpx;
	margin-left: 14rpx;
	position: relative;
	display: inline-block;
}
.attestation_itm {
	margin: 0 30rpx;
	margin-top: 40rpx;
}
.attestation_list {
	display: block;
	margin-bottom: 24rpx;
	border-radius: 16rpx;
	background-color: #EDEDED;
}
.attestation_list.cur {
	background-color: #2d2d2d;
}
.attestation_list_top {
	padding: 38rpx;
}
.attestation_list_bar {
	width: 100%;
	height: 2rpx;
	display: block;
	background: #ccc;
}
.attestation_list.cur .attestation_list_bar {
	background: -webkit-linear-gradient(315deg, rgba(235, 236, 171, 1) 0%, rgba(232, 235, 138, 1) 100%);
	background: linear-gradient(135deg, rgba(235, 236, 171, 1) 0%, rgba(232, 235, 138, 1) 100%);
}
.attestation_list_lf {
	float: left;
}
.attestation_list_lf .type {
	color: #333;
	display: block;
	font-size: 40rpx;
}
.attestation_list.cur .attestation_list_lf .type {
	color: #CACC78;
}
.attestation_list_lf .time {
	color: #999;
	display: block;
	margin-top: 1rpx;
	font-size: 26rpx;
}
.attestation_list.cur .attestation_list_lf .time {
	color: #F9F9F9;
}
.attestation_list_lr {
	float: right;
	margin-top: 13rpx;
}
.attestation_list_lr .i_font_status {
	width: 44rpx;
	height: 44rpx;
	display: block;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.attestation_list.cur .attestation_list_lr .i_font_status {
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.attestation_list_btn {
	line-height: 46rpx;
	padding: 22rpx 38rpx;
}
.attestation_list_btn .name {
	float: left;
	color: #999;
	display: block;
	font-size: 26rpx;
}
.attestation_list.cur .attestation_list_btn .name {
	color: #CACC78;
}
.attestation_list_btn .btn {
	width: 135rpx;
	height: 46rpx;
	float: right;
	color: #F9F9F9;
	display: block;
	font-size: 26rpx;
	text-align: center;
	line-height: 46rpx;
	border-radius: 8rpx;
	background-color: #D8D8D8;
}
.attestation_list.cur .attestation_list_btn .btn {
	color: #2D2D2D;
	background-color: #FFEFAE;
}

