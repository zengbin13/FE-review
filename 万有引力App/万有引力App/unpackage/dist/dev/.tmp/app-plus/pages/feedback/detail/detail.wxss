
.feedback_detail {
	padding: 30rpx;
}
.feedback_title {
	font-size: 28rpx;
	line-height: 40rpx;
	color: #333333;
	font-family: PingFangSC-Medium, PingFang SC;
	margin: 10rpx 0;
}
.theme_method {
	margin-top: 30rpx;
}
.theme_list {
	float: left;
	/* width: 220upx; */
	position: relative;
	margin-right: 20rpx;
	margin-bottom: 20rpx;
}
.theme_list:nth-child(3n) {
	margin-right: 0;
}
.theme_list .img {
	width: 216rpx;
	height: 216rpx;
	display: block;
	border-radius: 8rpx;
}

