
page {
	background-color: #F9F9F9;
}
.certification_top {
	height: 72rpx;
	padding: 0 32rpx;
	line-height: 72rpx;
	background-color: #FEFCEB;
}
.certification_top.hides {
	display: none;
}
.certification_top .xx {
	float: left;
	width: 38rpx;
	height: 35rpx;
	display: block;
	margin-top: 19rpx;
	margin-right: 18rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.certification_top .title {
	float: left;
	color: #F96A0E;
	display: block;
	font-size: 28rpx;
}
.certification_top .btn {
	float: right;
	width: 38rpx;
	height: 38rpx;
	display: block;
	margin-top: 18rpx;
	position: relative;
}
.certification_top .btn:after {
	content: "";
	width: 21rpx;
	height: 21rpx;
	display: block;
	margin: 0 auto;
	margin-top: 9rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.certification_bg {
	background-color: #fff;
}
.certification_itm .certification_list {
	padding: 23rpx 32rpx;
	padding-left: 0;
	margin-left: 32rpx;
	border-bottom: 1px solid #f1f1f1;
}
.certification_itm .certification_list:last-child {
	border-bottom: none;
}
.certification_itm .certification_list .name {
	width: 150rpx;
	float: left;
	color: #333;
	display: block;
	font-size: 34rpx;
}
.certification_itm .certification_list .int {
	width: 78%;
	float: left;
	color: #666;
	display: block;
	font-size: 34rpx;
}
.certification_itm .certification_list .sex {
	float: left;
	font-size: 34rpx;
	color: #ccc;
	width: 78%;
	position: relative;
}
.certification_itm .certification_list .sex:before {
	content: "";
	top: 8rpx;
	right: 0;
	width: 16rpx;
	height: 26rpx;
	display: block;
	position: absolute;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.certification_upload {
	padding: 23rpx 0;
}
.certification_upload .title {
	color: #333;
	font-size: 34rpx;
	padding-left: 32rpx;
}
.certification_uploadlist {
	margin-top: 30rpx;
}
.certification_uploadlist .img {
	width: 324rpx;
	height: 215rpx;
	display: block;
	border-radius: 8rpx;
}
.certification_uploadlist .certification_lf {
	float: left;
	width: 324rpx;
	margin-left: 34rpx;
	/* overflow: hidden; */
	position: relative;
}
.certification_uploadlist .certification_lf.other {
	margin-bottom: 20rpx;
}
.certification_uploadlist .certification_lr {
	float: right;
	width: 324rpx;
	overflow: hidden;
}
.certification_uploadlist .name {
	color: #333;
	display: block;
	font-size: 28rpx;
	text-align: center;
	margin-top: 17rpx;
}
.cirFree_btn {
	color: #fff;
	width: 455rpx;
	height: 93rpx;
	display: block;
	margin: 0 auto;
	font-size: 34rpx;
	text-align: center;
	line-height: 93rpx;
	margin-top: 200rpx;
	border-radius: 50rpx;
	background-color: #D8D8D8;
	box-shadow: -1rpx 10rpx 15rpx 3rpx rgba(51, 145, 255, 0.2);
}
.cirFree_btn.cur {
	background: -webkit-linear-gradient(330deg, rgba(28, 210, 197, 1) 0%, rgba(4, 186, 173, 1) 100%);
	background: linear-gradient(120deg, rgba(28, 210, 197, 1) 0%, rgba(4, 186, 173, 1) 100%);
}
.cirFree_itm {
	padding-bottom: 30rpx;
}
.certification_uploadlist .certification_lf .delete {
	top: -18rpx;
	right: -18rpx;
	z-index: 99;
	width: 36rpx;
	height: 36rpx;
	display: block;
	position: absolute;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}

