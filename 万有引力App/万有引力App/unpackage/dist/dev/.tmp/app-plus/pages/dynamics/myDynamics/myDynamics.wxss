.cirTab {
	border-bottom: 1px solid #E7E7E7;
}
.cirTab_list {
	color: #ccc;
	float: left;
	width: 33.3%;
	display: block;
	font-size: 28rpx;
	padding: 27rpx 0;
	text-align: center;
}
.cirTab_list.cur {
	color: #01B7AA;
}
.cirTab_list:after {
	content: "";
	top: 28rpx;
	width: 0;
	height: 4rpx;
	display: block;
	margin: 0 auto;
	position: relative;
	border-radius: 27rpx;
	background-color: #01B7AA;
}
.cirTab_list.cur:after {
	width: 20%;
	transition: all .5s;
	-moz-transition: all .5s;
	/* Firefox 4 */
	-webkit-transition: all .5s;
	/* Safari å’Œ Chrome */
	-o-transition: all .5s;
	/* Opera */
}
