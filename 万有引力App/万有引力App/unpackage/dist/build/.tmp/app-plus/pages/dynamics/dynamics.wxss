
page {
	background-color: #f1f4f9;
}
.navigation.status-bar {
	background-color: #01b7aa;
}
.dynamics_item {
	margin-top: 20rpx;
	padding: 0 30rpx;
	position: relative;
}
.dynamics_list {
	overflow: hidden;
	margin-bottom: 15rpx;
	border-radius: 10rpx;
	background-color: #fff;
	padding: 20rpx 7rpx 25rpx;
}
.dynamics_head {
	float: left;
	width: 79rpx;
	height: 79rpx;
	position: relative;
	margin-right: 16rpx;
}
.dynamics_head .img {
	width: 100%;
	height: 100%;
	display: block;
	border-radius: 50%;
}
.dynamics_menu {
	width: 85%;
	float: right;
	margin-top: 10rpx;
}
.dynamics_menu .dynamics_name {
	font-size: 30rpx;
	color: #3c3c3c;
	display: block;
}
.dynamics_menu .dynamics_name .img {
	width: 30rpx;
	height: 25rpx;
	margin-left: 9rpx;
	display: inline-block;
}
.dynamics_menu .dynamics_img {
	float: left;
	width: 196rpx;
	height: 196rpx;
	display: block;
	overflow: hidden;
	border-radius: 10rpx;
}
.dynamics_menu .dynamics_img .img {
	width: 196rpx;
	height: 196rpx;
	display: block;
}
.dynamics_menu .dynamics_itm {
	width: 60%;
	float: right;
}
.dynamics_menu .dynamics_itm.cur {
	width: 100%;
	float: initial;
}
.dynamics_menu .dynamics_itm .title {
	color: #b3b2b3;
	font-size: 24rpx;
	line-height: 160%;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.dynamics_menu .dynamics_itm .title.cur {
	color: #feb932;
}
.dynamics_menu .dynamics_itm .btn {
	float: right;
	color: #fff;
	width: 59px;
	height: 22px;
	display: block;
	font-size: 12px;
	line-height: 22px;
	text-align: center;
	border-radius: 11px;
	margin-top: 35rpx;
	margin-right: 20rpx;
	background-color: #01b7aa;
}
.dynamics_menu .dynamics_itm .nivo {
	float: right;
	margin-top: 30rpx;
	margin-right: 20rpx;
}
.dynamics_menu .dynamics_itm .nivo .num {
	float: left;
	color: #817e8a;
	font-size: 24rpx;
	margin-right: 56rpx;
}
.dynamics_menu .dynamics_itm .nivo .num .i_news {
	top: 5rpx;
	width: 26rpx;
	height: 26rpx;
	position: relative;
	margin-right: 7rpx;
	display: inline-block;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.dynamics_menu .dynamics_itm .nivo .num1 {
	float: left;
	color: #817e8a;
	font-size: 24rpx;
}
.dynamics_menu .dynamics_itm .nivo .num1 .i_support {
	top: 5rpx;
	width: 26rpx;
	height: 26rpx;
	position: relative;
	margin-right: 7rpx;
	display: inline-block;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.dynamics_menu .dynamics_itm .nivo .num1 .i_support.cur {
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.dynamics_info {
	margin-top: 30rpx;
}

