
page {
	background-color: #f1f4f8;
}
.navigation.status-bar {
	background-color: #01b7aa;
}
.banner {
	padding: 0 30rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 320rpx;
}
.swiper-box {
	width: 100%;
	height: 364rpx;
	overflow: hidden;
	position: relative;
	border-radius: 20rpx;
}
.ind_ban {
	width: 100%;
	height: 364rpx;
	overflow: hidden;
	position: relative;
	border-radius: 20rpx;
	box-shadow: 1px 1px 10px 3px rgba(0, 138, 128, 0.15);
}
.ind_ban .img {
	width: 100%;
	height: 364rpx;
	display: block;
	border-radius: 20rpx;
}
.swiper-dots {
	width: 100%;
	bottom: 15rpx;
	text-align: center;
	position: absolute;
}
.swiper-dots .num {
	width: 14rpx;
	height: 14rpx;
	margin: 0 8rpx;
	border-radius: 50%;
	display: inline-block;
	background-color: #fff;
}
.swiper-dots .num.active {
	width: 40rpx;
	border-radius: 10rpx;
	background-color: #58e6dc;
}
.notice {
	height: 70rpx;
	margin-top: 15rpx;
	border-radius: 5rpx;
	background-color: #fff;
}
.uni-swiper-msg {
	height: 70rpx;
	-webkit-flex-wrap: nowrap !important;
	        flex-wrap: nowrap !important;
	display: -webkit-box !important;
	display: -webkit-flex !important;
	display: flex !important;
}
.notice_list {
	width: 100%;
	height: 70rpx;
	color: #545252;
	font-size: 26rpx;
	line-height: 72rpx;
}
.notice_tub {
	float: left;
	width: 37rpx;
	height: 37rpx;
	display: block;
	margin: 18rpx 15rpx 0;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.circle_item {
	padding: 0 30rpx;
	margin-top: 40rpx;
}
.circle_top .circle_title {
	float: left;
	color: #303030;
	font-size: 32rpx;
	font-weight: bold;
}
.circle_top .circle_title:before {
	content: "";
	top: 5rpx;
	width: 6rpx;
	height: 28rpx;
	position: relative;
	border-radius: 3rpx;
	margin-right: 15rpx;
	display: inline-block;
	background-color: #01b7aa;
}
.circle_top .circle_more {
	float: right;
	color: #797979;
	font-size: 24rpx;
	margin-top: 2rpx;
}
.circle_top .circle_more .i_more {
	top: 2rpx;
	width: 18rpx;
	height: 18rpx;
	margin-right: 8rpx;
	position: relative;
	display: inline-block;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.circle_item .circleImg {
	float: left;
	width: 169rpx;
	height: 166rpx;
	display: block;
	overflow: hidden;
}
.circle_item .circleImg .img {
	width: 169rpx;
	height: 166rpx;
	display: block;
	overflow: hidden;
	border-radius: 10rpx;
}
.circle_item .circleItm {
	width: 70%;
	float: left;
	margin-left: 18rpx;
}
.circle_item .circle_list {
	overflow: hidden;
	margin-top: 18rpx;
	border-radius: 10rpx;
	background-color: #fff;
	padding: 27rpx 30rpx 20rpx 23rpx;
}
.circle_item .circleItm .name {
	font-size: 30rpx;
	color: #3c3c3c;
	display: block;
}
.circle_item .circleItm .intro {
	color: #b3b2b3;
	font-size: 24rpx;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	line-height: 170%;
	margin-top: 0rpx;
}
.circle_item .circleItm .member {
	float: left;
	color: #b3b2b3;
	display: block;
	font-size: 26rpx;
}
.circle_item .circleItm .member .num {
	color: #06bebd;
	font-weight: bold;
}
.circle_item .circleItm .circle_btn {
	float: right;
	color: #fff;
	width: 119rpx;
	height: 44rpx;
	display: block;
	font-size: 24rpx;
	margin-top: -2rpx;
	line-height: 44rpx;
	text-align: center;
	border-radius: 22rpx;
	background-color: #01b7aa;
}
.circle_item .circleItm .circleNivo {
	margin-top: 10rpx;
}

