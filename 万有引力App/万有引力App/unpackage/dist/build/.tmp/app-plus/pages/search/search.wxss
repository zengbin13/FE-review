
.navigation.status-bar .tabar2 .cancel {
	color: #999 !important;
}
.circle_head {
	height: 106rpx;
}
.circle_head .search {
	width: 80%;
	height: 67rpx;
	margin: 20rpx 0;
	overflow: hidden;
	line-height: 67rpx;
	position: relative;
	border-radius: 34rpx;
	background-color: #F4F4F4;
}
.circle_head .search .img {
	float: left;
	width: 38rpx;
	height: 38rpx;
	display: block;
	padding: 14rpx 0;
	margin-left: 26rpx;
}
.circle_head .search .int {
	width: 80%;
	float: left;
	color: #333;
	height: 67rpx;
	display: block;
	font-size: 28rpx;
	line-height: 67rpx;
	margin-left: 15rpx;
}
.circle_head .navRight {
	top: 10rpx;
	left: 20rpx;
	position: relative;
}
.circle_item .circleImg {
	float: left;
	width: 169rpx;
	height: 166rpx;
	display: block;
	overflow: hidden;
}
.circle_item .circleImg .img {
	width: 169rpx;
	height: 166rpx;
	display: block;
	overflow: hidden;
	border-radius: 10rpx;
}
.circle_item .circleItm {
	width: 70%;
	float: left;
	margin-left: 18rpx;
}
.circle_item .circle_list {
	overflow: hidden;
	margin-top: 18rpx;
	border-radius: 10rpx;
	background-color: #fff;
	padding: 27rpx 30rpx 20rpx 23rpx;
}
.circle_item .circleItm .name {
	font-size: 30rpx;
	color: #3c3c3c;
	display: block;
}
.circle_item .circleItm .intro {
	color: #b3b2b3;
	font-size: 24rpx;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	line-height: 170%;
	margin-top: 0rpx;
}
.circle_item .circleItm .member {
	float: left;
	color: #b3b2b3;
	display: block;
	font-size: 26rpx;
}
.circle_item .circleItm .member .num {
	color: #06bebd;
	font-weight: bold;
}
.circle_item .circleItm .circle_btn {
	float: right;
	color: #fff;
	width: 119rpx;
	height: 44rpx;
	display: block;
	font-size: 24rpx;
	margin-top: -2rpx;
	line-height: 44rpx;
	text-align: center;
	border-radius: 22rpx;
	background-color: #01b7aa;
}
.circle_item .circleItm .circleNivo {
	margin-top: 10rpx;
}
.ser_result {
	padding: 0 20rpx;
}
.ser_result .ser_top .ser_tit {
	color: #333;
	font-size: 28rpx;
}
.ser_result .ser_box {
	margin-top: 50rpx;
}
.ser_result .ser_li {
	color: #999;
	float: left;
	height: 64rpx;
	margin-top: 20rpx;
	padding: 0 32rpx;
	font-size: 28rpx;
	line-height: 64rpx;
	margin-right: 20rpx;
	border-radius: 64rpx;
	background-color: #f2f2f2;
}
.ser_result .hot_serBox .ser_tit {
	float: left;
}
.ser_result .hot_serBox .ser_click {
	top: -12rpx;
	float: right;
	position: relative;
}
.ser_result .hot_serBox .ser_click .img {
	width: 24rpx;
	height: 28rpx;
	display: inline-block;
	vertical-align: middle;
}
.ser_result .hot_serBox .ser_click .hist_btn {
	color: #ff1919;
	font-size: 28rpx;
	margin-left: 9rpx;
}

