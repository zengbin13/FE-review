
page {
	background-color: #f9f9f9;
}
.setUp_list {
	margin-top: 18rpx;
	background-color: #fff;
	border-bottom: 1px solid #eee;
}
.setUp_list .link {
	padding: 30rpx;
}
.setUp_list:last-child {
	margin-top: 0;
}
.setUp_list .name {
	float: left;
	color: #333;
	display: block;
	font-size: 30rpx;
}
.setUp_list .img {
	float: right;
	width: 16rpx;
	height: 26rpx;
	display: block;
	margin-top: 8rpx;
}
.setUp_list .num {
	float: right;
	color: #999;
	display: block;
	font-size: 28rpx;
	margin-top: 2rpx;
	margin-right: 13rpx;
}
.setUp_btn {
	color: #333;
	width: 100%;
	height: 104rpx;
	font-size: 34rpx;
	margin-top: 36rpx;
	text-align: center;
	line-height: 104rpx;
	background-color: #fff;
	border-top: 1px solid #eee;
	border-bottom: 1px solid #eee;
}
.setUp {
	margin-top: 24rpx;
}

