
.sysInfo {
	top: 70rpx;
	position: relative;
}
.sysInfo_top {
	padding: 0 30rpx;
}
.sysInfo .sys_title {
	float: left;
	color: #333;
	display: block;
	font-size: 54rpx;
}
.sysInfo .sys_return {
	width: 31rpx;
	height: 30rpx;
	float: right;
	display: block;
	margin-top: 23rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.sysInfo_content {
	padding: 0 30rpx;
}
.sysInfo_content .sysInfo_time {
	font-size: 20rpx;
	color: #B2B2B2;
	display: block;
	margin-top: 20rpx;
}
.sysInfo_content .sysInfo_p {
	color: #5d5d5d;
	display: block;
	font-size: 26rpx;
	line-height: 170%;
	margin-top: 20rpx;
}

